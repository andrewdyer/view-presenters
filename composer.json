{
    "name": "andrewdyer/view-presenters",
    "description": "A clean, reusable solution for separating presentation logic from models and views in PHP applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Dyer",
            "homepage": "https://andrewdyer.rocks",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Anddye\\ViewPresenters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anddye\\ViewPresenters\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpunit/phpunit": "^10.5"
    },
    "scripts": {
        "cs": "php-cs-fixer fix",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
